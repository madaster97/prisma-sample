{
  "name": "prisma-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=./dist node dist/bin/www.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 server/public/**/* server/views/* dist/",
    "build": "npm run clean && tsc -p . && npm run copy-files",
    "dev": "nodemon server/bin/www.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
